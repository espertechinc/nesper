<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.espertech.com/schema/esper" elementFormDefault="qualified" xmlns:esper="http://www.espertech.com/schema/esper" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="esper-configuration">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element ref="esper:common" minOccurs="0"/>
					<xs:element ref="esper:compiler" minOccurs="0"/>
					<xs:element ref="esper:runtime" minOccurs="0"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="common">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element ref="esper:event-type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:variant-stream" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:auto-import" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:auto-import-annotations" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:method-reference" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:database-reference" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:variable" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:event-meta" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="logging" type="esper:common-logging-type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="time-source" type="esper:common-time-source-type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="execution" type="esper:common-execution-type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:event-type-auto-name" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="compiler">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element ref="esper:plugin-view" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:plugin-virtualdw" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:plugin-aggregation-function" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:plugin-aggregation-multifunction" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:plugin-singlerow-function" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:plugin-pattern-guard" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:plugin-pattern-observer" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:plugin-method-datetime" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:plugin-method-enum" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:bytecode" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:language" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:stream-selection" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:view-resources" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="logging" type="esper:compiler-logging-type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="expression" type="esper:compiler-expression-type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="execution" type="esper:compiler-execution-type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:scripts" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:serde-settings" minOccurs="0"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="runtime">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element ref="esper:plugin-loader" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:threading" minOccurs="0"/>
					<xs:element ref="esper:variables" minOccurs="0"/>
					<xs:element ref="esper:patterns" minOccurs="0"/>
					<xs:element ref="esper:match-recognize" minOccurs="0"/>
					<xs:element ref="esper:metrics-reporting" minOccurs="0"/>
					<xs:element ref="esper:exceptionHandling" minOccurs="0"/>
					<xs:element ref="esper:conditionHandling" minOccurs="0"/>
					<xs:element name="logging" type="esper:runtime-logging-type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="time-source" type="esper:runtime-time-source-type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="expression" type="esper:runtime-expression-type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="execution" type="esper:runtime-execution-type" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="propertyResolutionStyleEnum">
		<xs:restriction base="xs:token">
			<xs:enumeration value="case_sensitive"/>
			<xs:enumeration value="case_insensitive"/>
			<xs:enumeration value="distinct_case_insensitive"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="eventRepresentationEnum">
		<xs:restriction base="xs:token">
			<xs:enumeration value="map"/>
			<xs:enumeration value="objectarray"/>
			<xs:enumeration value="avro"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="event-type-auto-name">
		<xs:complexType>
			<xs:attribute name="package-name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="event-type">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element ref="esper:xml-dom" minOccurs="0"/>
					<xs:element ref="esper:avro" minOccurs="0"/>
					<xs:element ref="esper:util-map" minOccurs="0"/>
					<xs:element ref="esper:objectarray" minOccurs="0"/>
					<xs:element ref="esper:legacy-type" minOccurs="0"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="class" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="xml-dom">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="esper:namespace-prefix" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="esper:xpath-property" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="root-element-name" type="xs:string" use="required"/>
			<xs:attribute name="root-element-namespace" type="xs:string" use="optional"/>
			<xs:attribute name="default-namespace" type="xs:string" use="optional"/>
			<xs:attribute name="schema-resource" type="xs:string" use="optional"/>
			<xs:attribute name="schema-text" type="xs:string" use="optional"/>
			<xs:attribute name="event-sender-validates-root" type="xs:boolean" use="optional"/>
			<xs:attribute name="auto-fragment" type="xs:boolean" use="optional"/>
			<xs:attribute name="xpath-resolve-properties-absolute" type="xs:boolean" use="optional"/>
			<xs:attribute name="xpath-property-expr" type="xs:boolean" use="optional"/>
			<xs:attribute name="xpath-function-resolver" type="xs:string" use="optional"/>
			<xs:attribute name="xpath-variable-resolver" type="xs:string" use="optional"/>
			<xs:attribute name="start-timestamp-property-name" type="xs:string" use="optional"/>
			<xs:attribute name="end-timestamp-property-name" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="namespace-prefix">
		<xs:complexType>
			<xs:attribute name="prefix" type="xs:string" use="required"/>
			<xs:attribute name="namespace" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="xpath-property">
		<xs:complexType>
			<xs:attribute name="property-name" type="xs:string" use="required"/>
			<xs:attribute name="xpath" type="xs:string" use="required"/>
			<xs:attribute name="type" type="esper:xpath-type" use="required"/>
			<xs:attribute name="cast" type="xs:string" use="optional"/>
			<xs:attribute name="event-type-name" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="xpath-type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="number"/>
			<xs:enumeration value="string"/>
			<xs:enumeration value="boolean"/>
			<xs:enumeration value="node"/>
			<xs:enumeration value="nodeset"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="util-map">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="esper:map-property"/>
			</xs:sequence>
			<xs:attribute name="supertype-names" type="xs:string" use="optional"/>
			<xs:attribute name="start-timestamp-property-name" type="xs:string" use="optional"/>
			<xs:attribute name="end-timestamp-property-name" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="avro">
		<xs:complexType>
			<xs:attribute name="schema-text" type="xs:string" use="optional"/>
			<xs:attribute name="supertype-names" type="xs:string" use="optional"/>
			<xs:attribute name="start-timestamp-property-name" type="xs:string" use="optional"/>
			<xs:attribute name="end-timestamp-property-name" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="objectarray">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="esper:objectarray-property"/>
			</xs:sequence>
			<xs:attribute name="supertype-names" type="xs:string" use="optional"/>
			<xs:attribute name="start-timestamp-property-name" type="xs:string" use="optional"/>
			<xs:attribute name="end-timestamp-property-name" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="objectarray-property">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="class" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="map-property">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="class" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="auto-import">
		<xs:complexType>
			<xs:attribute name="import-name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="auto-import-annotations">
		<xs:complexType>
			<xs:attribute name="import-name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="method-reference">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element ref="esper:lru-cache" minOccurs="0"/>
					<xs:element ref="esper:expiry-time-cache" minOccurs="0"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="class-name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="legacy-type">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element ref="esper:method-property" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:field-property" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="accessor-style" type="esper:accessorStyleEnum" use="optional"/>
			<xs:attribute name="code-generation" type="esper:codeGenerationEnum" use="optional"/>
			<xs:attribute name="property-resolution-style" type="esper:propertyResolutionStyleEnum" use="optional"/>
			<xs:attribute name="factory-method" type="xs:string" use="optional"/>
			<xs:attribute name="copy-method" type="xs:string" use="optional"/>
			<xs:attribute name="start-timestamp-property-name" type="xs:string" use="optional"/>
			<xs:attribute name="end-timestamp-property-name" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="method-property">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="accessor-method" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="field-property">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="accessor-field" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="accessorStyleEnum">
		<xs:restriction base="xs:token">
			<xs:enumeration value="native"/>
			<xs:enumeration value="explicit"/>
			<xs:enumeration value="public"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="codeGenerationEnum">
		<xs:restriction base="xs:token">
			<xs:enumeration value="enabled"/>
			<xs:enumeration value="disabled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="database-reference">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element ref="esper:datasource-connection" minOccurs="0"/>
					<xs:element ref="esper:datasourcefactory-connection" minOccurs="0"/>
					<xs:element ref="esper:drivermanager-connection" minOccurs="0"/>
					<xs:element ref="esper:connection-settings" minOccurs="0"/>
					<xs:element ref="esper:connection-lifecycle" minOccurs="0"/>
					<xs:element ref="esper:lru-cache" minOccurs="0"/>
					<xs:element ref="esper:expiry-time-cache" minOccurs="0"/>
					<xs:element ref="esper:column-change-case" minOccurs="0"/>
					<xs:element ref="esper:metadata-origin" minOccurs="0"/>
					<xs:element ref="esper:sql-types-mapping" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="datasource-connection">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="esper:env-property" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="context-lookup-name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="datasourcefactory-connection">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="esper:env-property" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="class-name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="env-property">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="value" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="drivermanager-connection">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="esper:connection-arg" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="class-name" type="xs:string" use="required"/>
			<xs:attribute name="url" type="xs:string" use="required"/>
			<xs:attribute name="user" type="xs:string" use="optional"/>
			<xs:attribute name="password" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="connection-arg">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="value" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="connection-settings">
		<xs:complexType>
			<xs:attribute name="auto-commit" type="xs:boolean" use="optional"/>
			<xs:attribute name="catalog" type="xs:string" use="optional"/>
			<xs:attribute name="read-only" type="xs:boolean" use="optional"/>
			<xs:attribute name="transaction-isolation" type="xs:int" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="connection-lifecycle">
		<xs:complexType>
			<xs:attribute name="value" type="esper:connection-lifecycle-enum" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="connection-lifecycle-enum">
		<xs:restriction base="xs:token">
			<xs:enumeration value="pooled"/>
			<xs:enumeration value="retain"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ref-type-enum">
		<xs:restriction base="xs:token">
			<xs:enumeration value="weak"/>
			<xs:enumeration value="soft"/>
			<xs:enumeration value="hard"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="lru-cache">
		<xs:complexType>
			<xs:attribute name="size" type="xs:int" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="expiry-time-cache">
		<xs:complexType>
			<xs:attribute name="max-age-seconds" type="xs:double" use="required"/>
			<xs:attribute name="purge-interval-seconds" type="xs:double" use="required"/>
			<xs:attribute name="ref-type" type="esper:ref-type-enum" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="plugin-view">
		<xs:complexType>
			<xs:attribute name="namespace" type="xs:string" use="required"/>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="forge-class" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="plugin-virtualdw">
		<xs:complexType>
			<xs:attribute name="namespace" type="xs:string" use="required"/>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="forge-class" type="xs:string" use="required"/>
			<xs:attribute name="config" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="plugin-aggregation-function">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="forge-class" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="plugin-aggregation-multifunction">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element ref="esper:init-arg" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="function-names" type="xs:string" use="required"/>
			<xs:attribute name="forge-class" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="plugin-singlerow-function">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="function-class" type="xs:string" use="required"/>
			<xs:attribute name="function-method" type="xs:string" use="required"/>
			<xs:attribute name="value-cache" type="esper:valueCacheEnum" use="optional"/>
			<xs:attribute name="filter-optimizable" type="esper:filterOptimizableEnum" use="optional"/>
			<xs:attribute name="rethrow-exceptions" type="xs:boolean" use="optional"/>
			<xs:attribute name="event-type-name" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="plugin-method-datetime">
		<xs:complexType>
			<xs:attribute name="method-name" type="xs:string" use="required"/>
			<xs:attribute name="forge-class" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="plugin-method-enum">
		<xs:complexType>
			<xs:attribute name="method-name" type="xs:string" use="required"/>
			<xs:attribute name="forge-class" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="valueCacheEnum">
		<xs:restriction base="xs:token">
			<xs:enumeration value="enabled"/>
			<xs:enumeration value="configured"/>
			<xs:enumeration value="disabled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="filterOptimizableEnum">
		<xs:restriction base="xs:token">
			<xs:enumeration value="enabled"/>
			<xs:enumeration value="disabled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="plugin-pattern-guard">
		<xs:complexType>
			<xs:attribute name="namespace" type="xs:string" use="required"/>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="forge-class" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="plugin-pattern-observer">
		<xs:complexType>
			<xs:attribute name="namespace" type="xs:string" use="required"/>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="forge-class" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="plugin-loader">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element ref="esper:init-arg" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="esper:config-xml" minOccurs="0"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="class-name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="init-arg">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="value" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="bytecode">
		<xs:complexType>
			<xs:attribute name="include-comments" type="xs:boolean" use="optional"/>
			<xs:attribute name="include-debugsymbols" type="xs:boolean" use="optional"/>
			<xs:attribute name="attach-epl" type="xs:boolean" use="optional"/>
			<xs:attribute name="attach-module-epl" type="xs:boolean" use="optional"/>
			<xs:attribute name="attach-pattern-epl" type="xs:boolean" use="optional"/>
			<xs:attribute name="allow-subscriber" type="xs:boolean" use="optional"/>
			<xs:attribute name="allow-inlined-class" type="xs:boolean" use="optional"/>
			<xs:attribute name="instrumented" type="xs:boolean" use="optional"/>
			<xs:attribute name="bus-modifier-event-type" type="esper:busModifierEnum" use="optional"/>
			<xs:attribute name="access-modifier-context" type="esper:accessModifierEnum" use="optional"/>
			<xs:attribute name="access-modifier-event-type" type="esper:accessModifierEnum" use="optional"/>
			<xs:attribute name="access-modifier-expression" type="esper:accessModifierEnum" use="optional"/>
